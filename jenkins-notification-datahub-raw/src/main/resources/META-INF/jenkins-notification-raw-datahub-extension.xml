<extension xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.hybris.com/schema/"
		   xsi:schemaLocation="http://www.hybris.com/schema/ datahub-metadata-schema-1.1.0.xsd" name="jenkins-notification-datahub-raw">

	<dependencies>
		<dependency>
			<extension>jenkins-notification-datahub-canonical</extension>
		</dependency>
	</dependencies>
	<rawItems>
		<item>
			<type>JenkinsNotificationRawItem</type>
			<attributes>
				<attribute>
					<name>name</name>
				</attribute>
				<attribute>
					<name>url</name>
				</attribute>
				<attribute>
					<name>buildUrl</name>
				</attribute>
				<attribute>
					<name>buildFullUrl</name>
				</attribute>
				<attribute>
					<name>buildNumber</name>
				</attribute>
				<attribute>
					<name>buildPhase</name>
				</attribute>
				<attribute>
					<name>buildStatus</name>
				</attribute>
			</attributes>
		</item>
	</rawItems>
	<canonicalItems>
		<item>
			<type>JenkinsNotificationCanonicalItem</type>
			<attributes>
				<attribute>
					<name>name</name>
					<transformations>
						<transformation>
							<rawSource>JenkinsNotificationRawItem</rawSource>
							<expression>name</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>url</name>
					<transformations>
						<transformation>
							<rawSource>JenkinsNotificationRawItem</rawSource>
							<expression>url</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>buildUrl</name>
					<transformations>
						<transformation>
							<rawSource>JenkinsNotificationRawItem</rawSource>
							<expression>buildUrl</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>buildFullUrl</name>
					<transformations>
						<transformation>
							<rawSource>JenkinsNotificationRawItem</rawSource>
							<expression>buildFullUrl</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>buildNumber</name>
					<transformations>
						<transformation>
							<rawSource>JenkinsNotificationRawItem</rawSource>
							<expression>buildNumber</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>buildPhase</name>
					<transformations>
						<transformation>
							<rawSource>JenkinsNotificationRawItem</rawSource>
							<expression>buildPhase</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>buildStatus</name>
					<transformations>
						<transformation>
							<rawSource>JenkinsNotificationRawItem</rawSource>
							<expression>buildStatus</expression>
						</transformation>
					</transformations>
				</attribute>
			</attributes>
		</item>
	</canonicalItems>
</extension>
